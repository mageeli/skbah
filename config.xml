<?xml version='1.0' encoding='utf-8'?>
<widget id="net.ogail.skbah" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>skbah</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <plugin name="cordova-plugin-device" spec="1.1.5" />
    <plugin name="cordova-plugin-device-motion" spec="1.2.4" />
    <plugin name="cordova-plugin-device-orientation" spec="1.0.6" />
    <plugin name="cordova-plugin-inappbrowser" spec="1.7.0" />
    <plugin name="cordova-plugin-splashscreen" spec="4.0.2" />
    <plugin name="cordova-plugin-vibration" spec="2.1.4" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.2" />

    <splash src="2048-2732.png"/>
    <preference name="SplashScreenDelay" value="15000"/>
    <preference name="Orientation" value="portrait" />
    <platform name="android">
        <splash src="res/screen/android/splashscreen-480x640.png" density="land-hdpi"/>
        <splash src="res/screen/android/splashscreen-320x426.png" density="land-ldpi"/>
        <splash src="res/screen/android/splashscreen-320x470.png" density="land-mdpi"/>
        <splash src="res/screen/android/android-720x960.png" density="land-xhdpi"/>
    </platform>

    <platform name="ios">
        <splash src="res/screen/ios/splashscreen-320x480.png" width="320" height="480"/>
        <splash src="res/screen/ios/splashscreen-640x960.png" width="640" height="960"/>
        <splash src="res/screen/ios/768-1024.png" width="768" height="1024"/>
        <splash src="res/screen/ios/1536-2048.png" width="1536" height="2048"/>
        <splash src="res/screen/ios/Default-Landscape~ipad.png" width="1024" height="768"/>
        <splash src="res/screen/ios/Default-Landscape@2x~ipad.png" width="2048" height="1536"/>
        <splash src="res/screen/ios/splashscreen-640x1136.png" width="640" height="1136"/>
        <splash src="res/screen/ios/750-1334.png" width="750" height="1334"/>
        <splash src="res/screen/ios/1242-2208.png" width="1242" height="2208"/>
        <splash src="res/screen/ios/Default-Landscape-736h.png" width="2208" height="1242"/>
    </platform>

    <access origin="http://sakbah.ogail.net*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>

    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
    <plugin name="com.filfatstudios.spinnerdialog" spec="https://github.com/filfat-Studios-AB/cordova-plugin-spinnerdialog" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.3" />
</widget>
